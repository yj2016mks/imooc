#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_html@1.0.0@html/bin/html.js" "$@"
  ret=$?
else 
  node  "$basedir/../_html@1.0.0@html/bin/html.js" "$@"
  ret=$?
fi
exit $ret
